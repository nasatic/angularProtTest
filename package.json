{
  "name": "web-uat-tests",
  "version": "0.0.0",
  "license": "MIT",
  "description": "To demostrate cucumber tests for angular/non angular apps with allure report framework",
  "homepage": "http://www.way2automation.com/angularjs-protractor/registeration/#/login",
  "keywords": [
    "protractor",
    "cucumber",
    "typescript",
    "angular",
    "angularjs",
    "testing",
    "behaviour driven development",
    "bdd",
    "selenium",
    "webdriverJS",
    "gherkin",
    "automation testing"
  ],
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "build": "tsc -p .",
    "clean": "rm -rf node_modules && npm install -g && npm install -g protractor",
    "chrome": "npm uninstall -g chromedriver && npm install -g chromedriver",
    "wdUpdate": "webdriver-manager update",
    "wdStart": "webdriver-manager start",
    "test": "npm run wdUpdate && protractor typeScript/config/config.js",
    "kill": "del cucumberReport.*.json, taskkill chromedriver.exe",
    "fix-memory-limit": "cross-env LIMIT=8192 increase-memory-limit",
    "git": "git pull origin master && git push origin master",
    "dev": "npm test && npm run kill"
  },
  "private": true,
